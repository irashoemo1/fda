		<!--also need to make a route for all the industry names and codes so people can know-->
		<!-- Make the enforcement one output multiple things like the events one -->
		<!-- bootstrap to make it look good and put it into cards -->
		<section id="enforcementInput">
			<h2 class="heading">Input a reason for recall:</h2>
			<input type=text placeholder="Enter a Food or Drink" id="reasonInput">
			<input type=text placeholder="Enter a State's initials" id="stateInput">
			<input type=text placeholder="Enter the from date yyyymmdd" id="fromDateInput">
			<input type=text placeholder="Enter the to date yyyymmdd" id="toDateInput">
			<button class="btn btn-primary" id="byReason">Search by Reason</button>
		</section>

		<section id="eventInput">
			<h2 class="heading">Input for event:</h2>
			<input type=text placeholder="Enter a Food or Drink" id="industryNameInput">
			<input type=text placeholder="Enter a reactions" id="reactionsInput">
			<input type=text placeholder="Enter a start date yyyymmdd" id="startDateInput">
			<button class="btn btn-primary" id="byEffect">Search by Effect</button>
		</section>
		
		<section class="pagination container">
			<section class="row row-cols-1 row-cols-md-4">
				<h2 class="page col-lg-4">Previous page: 
					<span class="page-number2"></span>
				</h2>
				<h2 class="page col-lg-4">Next page: 
					<span class="page-number1"></span>
				</h2>
				<section class="btn-group">
					<button id="prev-page" class="page-changer btn btn-primary">Previous page</button>
					<button id="next-page" class="page-changer btn btn-primary">Next page</button>
				</section>
			</section>
		</section>

